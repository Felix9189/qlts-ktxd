}

.grid-container-insert {
  display: grid;
  grid-template-columns: 50% 50%;
  background-color: #2196F3;
  padding: 10px;
}

.grid-item-insert {
  background-color: rgba(255, 255, 255, 0.8);
  border: 1px solid rgba(0, 0, 0, 0.8);
  padding: 20px;
  font-size: 20px;
  text-align: center;
}
.item1 ,.item2, .item5, .item6 {
	grid-column: 1 / span 2;
}
.allData {
  margin-top: 60px;
}
label {
